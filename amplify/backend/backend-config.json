{
  "api": {
    "kakaomsgreservation": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "defaultAuthentication": {
            "authenticationType": "API_KEY",
            "apiKeyConfig": {
              "apiKeyExpirationDays": 365,
              "apiKeyExpirationDate": "2022-12-07T12:07:07.200Z",
              "description": ""
            }
          },
          "additionalAuthenticationProviders": []
        }
      }
    }
  },
  "function": {
    "kakaoTokenRefreshLambda": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "kakaomsgreservation",
          "attributes": [
            "GraphQLAPIIdOutput",
            "GraphQLAPIEndpointOutput",
            "GraphQLAPIKeyOutput"
          ]
        }
      ]
    },
    "kakaoMsgSenderLambda": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "kakaomsgreservation",
          "attributes": [
            "GraphQLAPIIdOutput",
            "GraphQLAPIEndpointOutput",
            "GraphQLAPIKeyOutput"
          ]
        },
        {
          "category": "function",
          "resourceName": "kakaoTokenRefreshLambda",
          "attributes": [
            "Name"
          ]
        }
      ]
    }
  }
}